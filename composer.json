{
    "name": "owlchester/laravel-translation-manager",
    "description": "Enhanced Laravel Translation Manager",
    "keywords": [
        "laravel",
        "translations",
        "translator"
    ],
    "homepage": "https://github.com/owlchester/laravel-translation-manager",
    "license": "MIT",
    "authors": [
        {
            "name": "Vladimir Schneider",
            "email": "vladimir.schneider@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Jay Payne",
            "email": "hello@kanka.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.3.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/translation": "^10.0|^11.0|^12.0",
        "symfony/finder": "^7.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^11.5.3"
    },
    "autoload": {
        "classmap": [
            "src/Classes"
        ],
        "files": [
            "src/Support/helpers.php"
        ],
        "psr-4": {
            "Vsch\\TranslationManager\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ],
        "psr-4": {
        }
    },
    "scripts": {
        "post-update-cmd": [
        ]
    },
    "minimum-stability": "stable"
}
