{
    "name": "vsch/laravel-translation-manager",
    "description": "Enhanced Laravel Translation Manager",
    "keywords": [
        "laravel",
        "translations",
        "translator"
    ],
    "homepage": "https://github.com/vsch/laravel-translation-manager",
    "license": "MIT",
    "authors": [
        {
            "name": "Vladimir Schneider",
            "email": "vladimir.schneider@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "illuminate/support": "^8.0",
        "laravelcollective/html": "^6.2",
        "illuminate/translation": "^8.0",
        "doctrine/dbal": "^3.0",
        "symfony/finder": "^5.1"
    },
    "require-dev": {
        "phpspec/phpspec": "^7.0",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.2",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "classmap": [
            "src/Classes"
        ],
        "files": [
            "src/Support/helpers.php"
        ],
        "psr-4": {
            "Vsch\\TranslationManager\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ],
        "psr-4": {
        }
    },
    "scripts": {
        "post-update-cmd": [
        ]
    },
    "minimum-stability": "stable"
}
